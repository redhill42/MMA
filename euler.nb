  3.73041015888896*^9}, {3.730410211400827*^9, 3.730410269200821*^9}, {
  3.7304540135736237`*^9, 3.7304540306675987`*^9}, {3.73045408214038*^9, 
  3.7304540904114637`*^9}, {3.73051405283687*^9, 3.7305141688130302`*^9}, {
