Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", "countDigits", "}"}], ",", "\n", "    ", 
   RowBox[{
    RowBox[{
     RowBox[{"countDigits", "[", "x_", "]"}], ":=", 
     RowBox[{"0", "/;", 
      RowBox[{"IntegerQ", "[", 
       RowBox[{"Sqrt", "[", "x", "]"}], "]"}]}]}], ";", "\n", "    ", 
    RowBox[{
     RowBox[{"countDigits", "[", "x_", "]"}], ":=", "\n", "        ", 
     RowBox[{"Total", "@", 
      RowBox[{"IntegerDigits", "@", 
       RowBox[{"Floor", "[", 
        RowBox[{
         RowBox[{"Sqrt", "[", "x", "]"}], "*", 
         RowBox[{"10", "^", "99"}]}], "]"}]}]}]}], ";", "\n", "        ", 
