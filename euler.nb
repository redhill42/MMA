Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"cube", ",", "pairs", ",", "check", ",", "combination"}], "}"}], 
   ",", "\n", "    ", 
   RowBox[{
    RowBox[{"cube", "=", 
     RowBox[{
      RowBox[{"Subsets", "[", 
       RowBox[{
        RowBox[{"Range", "[", 
         RowBox[{"0", ",", "9"}], "]"}], ",", 
        RowBox[{"{", "6", "}"}]}], "]"}], "/.", " ", 
      RowBox[{"9", "\[Rule]", "6"}]}]}], ";", "\n", "    ", 
    RowBox[{"pairs", "=", 
     RowBox[{
      RowBox[{"Select", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"FromDigits", "[", 
          RowBox[{"#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
           "]"}], "\[LessEqual]", 
         RowBox[{"FromDigits", "[", 
          RowBox[{"#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}],
           "]"}]}], "&"}], "]"}], "@", 
      RowBox[{"Catenate", "@", 
       RowBox[{"Outer", "[", 
        RowBox[{"List", ",", "cube", ",", "cube", ",", "1"}], "]"}]}]}]}], 
    ";", "\n", "  ", 
    RowBox[{
     RowBox[{
      RowBox[{"check", "[", "digits_", "]"}], "[", 
      RowBox[{"{", 
       RowBox[{"a_", ",", "b_"}], "}"}], "]"}], ":=", "\n", "        ", 
     RowBox[{"AllTrue", "[", 
      RowBox[{"digits", ",", "\n", "            ", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"MemberQ", "[", 
           RowBox[{"a", ",", 
            RowBox[{
            "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
           "]"}], "&&", 
          RowBox[{"MemberQ", "[", 
           RowBox[{"b", ",", 
            RowBox[{
            "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
           "]"}]}], "||", "\n", "            ", 
         RowBox[{
          RowBox[{"MemberQ", "[", 
           RowBox[{"b", ",", 
            RowBox[{
            "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
           "]"}], "&&", 
          RowBox[{"MemberQ", "[", 
           RowBox[{"a", ",", 
            RowBox[{
            "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
           "]"}]}]}], "&"}]}], "]"}]}], ";", "\n", "    ", 
    RowBox[{
     RowBox[{"combination", "[", "numbers_", "]"}], ":=", "\n", "        ", 
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"digits", "=", 
         RowBox[{
          RowBox[{"Map", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"PadLeft", "[", 
              RowBox[{
               RowBox[{"IntegerDigits", "[", "#", "]"}], ",", "2"}], "]"}], 
             "&"}], ",", "numbers"}], "]"}], "/.", " ", 
          RowBox[{"9", "\[Rule]", "6"}]}]}], "}"}], ",", "\n", "            ", 
       RowBox[{"Select", "[", 
        RowBox[{"pairs", ",", 
         RowBox[{"check", "[", "digits", "]"}]}], "]"}]}], "]"}]}], ";", "\n",
     "    ", 
    RowBox[{"Length", "@", 
     RowBox[{"combination", "[", 
      RowBox[{
       RowBox[{"Range", "[", "9", "]"}], "^", "2"}], "]"}]}]}]}], "\n", 
  "]"}]], "Input",ExpressionUUID->"2b0435c1-fae1-4c69-9260-516572dffc73"],

