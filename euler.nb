    RowBox[{
     RowBox[{
      RowBox[{"polygonalQ", "[", "8", "]"}], "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
      FractionBox[
       RowBox[{
        SqrtBox[
         RowBox[{
          RowBox[{"3", "x"}], "+", "1"}]], "+", "1"}], "3"], "]"}]}], ";", 
    "\n", "    ", "\n", "    ", 
    RowBox[{
     RowBox[{"cyclic", "[", "polygonals_", "]"}], ":=", "\n", "        ", 
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"leading", "=", 
