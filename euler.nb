        RowBox[{"Min", "[", 
         RowBox[{"m", ",", "n"}], "]"}]}], "]"}]}], ";", "\n", "    ", "\n", 
     "    ", 
     RowBox[{
      RowBox[{"chain", "[", "_", "]"}], "=", "0"}], ";", "\n", "    ", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"chain", "[", "i", "]"}], "\[Equal]", "0"}], ",", "\n", 
         "        ", 
         RowBox[{"With", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"nx", "=", 
             RowBox[{"NestWhile", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"(", 
