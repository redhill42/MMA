        RowBox[{"b", "-", "1"}], ")"}]}], 
      RowBox[{"n", 
       RowBox[{"(", 
        RowBox[{"n", "-", "1"}], ")"}]}]], "\[Equal]", 
     FractionBox["1", "2"]}], "&&", 
    RowBox[{"0", "<", "b", "<", 
     SuperscriptBox["10", "12"], "<", "n"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"b", ",", "n"}], "}"}], ",", "Integers"}], "]"}]], "Input",
