Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "pythagorean", ",", "pyth", ",", "triplet", ",", "count", ",", "path", 
     ",", "total"}], "}"}], ",", "\n", "    ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Generate", " ", "the", " ", "table", " ", "for", " ", "all", " ", 
     "Pythagorean", " ", "triplet"}], " ", "*)"}], "\n", "    ", 
   RowBox[{
    RowBox[{
     RowBox[{"pyth", "[", "_", "]"}], "=", 
     RowBox[{"{", "}"}]}], ";", "\n", "    ", 
    RowBox[{
     RowBox[{"pythagorean", "[", "max_", "]"}], ":=", "\n", "        ", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"GCD", "[", 
           RowBox[{"m", ",", "n"}], "]"}], "\[Equal]", "1"}], ",", "\n", 
         "            ", 
         RowBox[{"With", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"a", "=", 
              RowBox[{
               RowBox[{"m", "^", "2"}], "-", 
               RowBox[{"n", "^", "2"}]}]}], ",", 
             RowBox[{"b", "=", 
              RowBox[{"2", "m", " ", "n"}]}]}], "}"}], ",", "\n", 
           "                ", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"a", ">", "max"}], "&&", 
               RowBox[{"b", ">", "max"}]}], ",", 
              RowBox[{"Break", "[", "]"}]}], "]"}], ";", "\n", 
            "                ", 
            RowBox[{
             RowBox[{"pyth", "[", "a", "]"}], "=", 
             RowBox[{"Append", "[", 
              RowBox[{
               RowBox[{"pyth", "[", "a", "]"}], ",", "b"}], "]"}]}], ";", 
            "\n", "                ", 
            RowBox[{
             RowBox[{"pyth", "[", "b", "]"}], "=", 
             RowBox[{"Append", "[", 
              RowBox[{
               RowBox[{"pyth", "[", "b", "]"}], ",", "a"}], "]"}]}]}]}], 
          "]"}]}], "]"}], ",", "\n", "            ", 
       RowBox[{"{", 
        RowBox[{"m", ",", "Infinity"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", 
         RowBox[{
          RowBox[{"BitAnd", "[", 
           RowBox[{"m", ",", "1"}], "]"}], "+", "1"}], ",", 
         RowBox[{"m", "-", "1"}], ",", "2"}], "}"}]}], "]"}]}], ";", "\n", 
    "\n", "    ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Find", " ", "all", " ", "Pythagorean", " ", "triplet", " ", "for", " ", 
      "the", " ", "given", " ", "number"}], " ", "*)"}], "\n", "    ", 
    RowBox[{
     RowBox[{"triplet", "[", "n_", "]"}], ":=", "\n", "        ", 
     RowBox[{
      RowBox[{"Select", "[", 
       RowBox[{
        RowBox[{"0", "<", "#", "<", 
         RowBox[{"2", "n"}]}], "&"}], "]"}], "@", 
      RowBox[{"Flatten", "@", 
       RowBox[{
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"n", "/", "#"}], ")"}], 
           RowBox[{"pyth", "[", "#", "]"}]}], "&"}], "]"}], "@", 
        RowBox[{"Divisors", "[", "n", "]"}]}]}]}]}], ";", "\n", "        ", 
    "\n", "    ", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
       "Count", " ", "the", " ", "paths", " ", "that", " ", "confirm", " ", 
        "to", " ", "the", " ", "formula", " ", 
        RowBox[{"a", "^", "2"}]}], "+", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"b", "+", "c"}], ")"}], "^", "2"}], " ", "for", " ", 
        "a"}]}], "\[ThinSpace]", "\[GreaterEqual]", "\[ThinSpace]", "b", 
      "\[ThinSpace]", "\[GreaterEqual]", "\[ThinSpace]", "c", 
      "\[VeryThinSpace]", ">", "\[ThinSpace]", "0"}], " ", "*)"}], "\n", 
    "    ", 
    RowBox[{
     RowBox[{
      RowBox[{"count", "[", "n_", "]"}], "[", "m_", "]"}], ":=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"m", ">", "n"}], ",", 
       RowBox[{
        RowBox[{"Floor", "[", 
         RowBox[{"n", "-", 
          RowBox[{"m", "/", "2"}]}], "]"}], "+", "1"}], ",", 
       RowBox[{"Floor", "[", 
        RowBox[{"m", "/", "2"}], "]"}]}], "]"}]}], ";", "\n", "\n", "    ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Count", " ", "the", " ", "Pythagorean", " ", "triplet", " ", "for", " ",
       "the", " ", "given", " ", "number"}], " ", "*)"}], "\n", "    ", 
    RowBox[{
     RowBox[{"path", "[", "n_", "]"}], ":=", 
     RowBox[{"Total", "@", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{"count", "[", "n", "]"}], "]"}], "@", 
       RowBox[{"triplet", "[", "n", "]"}]}]}]}], ";", "\n", "        ", "\n", 
    "    ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Count", " ", "all", " ", "until", " ", "total", " ", "exceeds", " ", 
      "M"}], " ", "*)"}], "\n", "    ", 
    RowBox[{"pythagorean", "[", "10000", "]"}], ";", "\n", "    ", 
