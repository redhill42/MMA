Cell[TextData[{
 StyleBox["public class",
  FontColor->RGBColor[0, 0, 1]],
 " Problem141 {\n    ",
 StyleBox["private final static long",
  FontColor->RGBColor[0, 0, 1]],
 " LIMIT = 1_000_000_000_000L;\n\n    ",
 StyleBox["private static boolean",
  FontColor->RGBColor[0, 0, 1]],
 " isSquare(",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " n) {\n        ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " r = (",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 ")Math.sqrt(n);\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " r * r == n;\n    }\n\n    ",
 StyleBox["private static long",
  FontColor->RGBColor[0, 0, 1]],
 " gcd(",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " a, ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " b) {\n        ",
 StyleBox["while",
  FontColor->RGBColor[0, 0, 1]],
 " (b != 0) {\n            ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " m = a % b;\n            a = b;\n            b = m;\n        }\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " a;\n    }\n\n    ",
 StyleBox["public",
  FontColor->RGBColor[0, 0, 1]],
 " ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " solve() {\n        ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " sum = 0;\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " a = 2; a <= 10000; a++) {\n            ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " b = 1; b < a; b++) {\n                ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (gcd(a, b) != 1)\n                    ",
 StyleBox["continue",
  FontColor->RGBColor[0, 0, 1]],
 ";\n                ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " c = 1; c < LIMIT; c++) {\n                    ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " n = a*a*a*b*c*c + b*b*c;\n                    ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (n > LIMIT)\n                        ",
 StyleBox["break",
  FontColor->RGBColor[0, 0, 1]],
 ";\n                    ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (isSquare(n))\n                        sum += n;\n                }\n      \
      }\n        }\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " sum;\n    }\n\n    ",
 StyleBox["public static void",
  FontColor->RGBColor[0, 0, 1]],
 " main(String[] args) {\n        System.out.println(new \
Problem141().solve());\n    }\n}"
}], "Program",ExpressionUUID->"9c168892-d66f-41b1-b3ef-1bc0cdff6495"],

