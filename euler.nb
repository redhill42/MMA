  FontColor->RGBColor[0, 0, 1]],
 "[LIMIT + 1];\n    ",
 StyleBox["final int",
  FontColor->RGBColor[0, 0, 1]],
 "[] chain = ",
 StyleBox["new int",
  FontColor->RGBColor[0, 0, 1]],
 "[LIMIT + 1];\n\n    ",
 StyleBox["void",
  FontColor->RGBColor[0, 0, 1]],
 " init() {\n        Arrays.fill(divsum, 1);\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " q = 2; q <= Math.sqrt(LIMIT); q++) {\n            ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " c = q; c*q <= LIMIT; c++) {\n                divsum[c*q] += c+q;\n         \
   }\n        }\n    }\n\n    ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " minval(",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " a) {\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " start = a, m = a;\n        ",
 StyleBox["do",
  FontColor->RGBColor[0, 0, 1]],
 " {\n            m = Math.min(m, a);\n            a = divsum[a];\n        } ",
 StyleBox["while",
  FontColor->RGBColor[0, 0, 1]],
 " (a != start);\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " m;\n    }\n\n    ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " solve() {\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " L = 1;\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " maxlen = 0, minval = 0;\n\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 3; i <= LIMIT; i++) {\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (chain[i] != 0)\n                ",
 StyleBox["continue",
  FontColor->RGBColor[0, 0, 1]],
 ";\n\n            ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " nx = i;\n            ",
 StyleBox["while",
  FontColor->RGBColor[0, 0, 1]],
 " (nx <= LIMIT && chain[nx] == 0) {\n                chain[nx] = L++;\n      \
          nx = divsum[nx];\n            }\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (nx <= LIMIT && chain[nx] >= chain[i]) {\n                ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " len = L - chain[nx];\n                ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (len > maxlen) {\n                    maxlen = len;\n                    \
minval = minval(nx);\n                }\n            }\n        }\n\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " minval;\n    }\n\n    ",
 StyleBox["public static",
  FontColor->RGBColor[0, 0, 1]],
 " ",
 StyleBox["void",
  FontColor->RGBColor[0, 0, 1]],
 " main(String[] args) {\n        AmicableChain solver = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " AmicableChain();\n        solver.init();\n        \
System.out.println(solver.solve());\n    }\n}"
