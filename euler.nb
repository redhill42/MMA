Cell[TextData[{
 StyleBox["import",
  FontColor->RGBColor[0, 0, 1]],
 " java.util.BitSet;\n\n",
 StyleBox["public class",
  FontColor->RGBColor[0, 0, 1]],
 " PrimeGenerator {\n    ",
 StyleBox["static final int",
  FontColor->RGBColor[0, 0, 1]],
 " LIMIT = 100000000;\n    BitSet primes = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " BitSet(LIMIT);\n\n    PrimeGenerator() {\n        primes.set(2);\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 3; i <= LIMIT; i += 2)\n            primes.set(i);\n        \n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " sq = (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 ")Math.sqrt(LIMIT);\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 3; i <= sq; i += 2) {\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (primes.get(i))\n                ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (int j = i*i; j <= LIMIT; j += i)\n                    primes.clear(j);\n  \
      }\n    }\n\n    ",
 StyleBox["boolean",
  FontColor->RGBColor[0, 0, 1]],
 " check(",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " n) {\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " sq = (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 ")Math.sqrt(n);\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 1; i <= sq; i++) {\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (n % i == 0 && !primes.get(i + n / i))\n                ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " ",
 StyleBox["false",
  FontColor->RGBColor[0, 0, 1]],
 ";\n        }\n        ",
 StyleBox["return true",
  FontColor->RGBColor[0, 0, 1]],
 ";\n    }\n\n    ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " solve() {\n        ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " sum = 0;\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 2; i > 0; i = primes.nextSetBit(i+1)) {\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (check(i-1))\n                sum += i-1;\n        }\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " sum;\n    }\n\n    ",
 StyleBox["public static void",
  FontColor->RGBColor[0, 0, 1]],
 " main(String[] args) {\n        PrimeGenerator solver = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " PrimeGenerator();\n        System.out.println(solver.solve());\n    }\n}"
}], "Program",
