       RowBox[{"group", ",", 
        RowBox[{"{", "}"}]}], "]"}]}], ";", "\n", "    ", "\n", "    ", 
     RowBox[{
      RowBox[{"countPrimes", "[", 
       RowBox[{
        RowBox[{"{", "}"}], ",", "primes_"}], "]"}], ":=", "\n", "        ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"pandigitalPrimesQ", "[", "primes", "]"}], ",", "\n", 
