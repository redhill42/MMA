     RowBox[{"Import", "[", 
      RowBox[{
      "\"\<https://projecteuler.net/project/resources/p099_base_exp.txt\>\"", 
       ",", "\"\<CSV\>\""}], "]"}]}], ";", "\n", "    ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"MaximalBy", "[", "Last", "]"}], "@", 
