       RowBox[{"7", 
        RowBox[{"(", 
         RowBox[{
          SqrtBox["23"], "+", "4"}], ")"}]}], "7"], "=", 
      RowBox[{"8", "+", 
       SqrtBox["23"], "-", "4"}]}]}]}], 
