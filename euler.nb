Cell[BoxData[
 RowBox[{
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "matrix", ",", " ", "rows", ",", " ", "cols", ",", " ", "index", ",", 
