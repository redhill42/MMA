     RowBox[{"Flatten", "@", 
      RowBox[{"Import", "[", 
       RowBox[{
       "\"\<https://projecteuler.net/project/resources/p089_roman.txt\>\"", 
        ",", "\"\<Table\>\""}], "]"}]}]}], ";", "\n", "    ", 
    RowBox[{"Off", "[", 
     RowBox[{"FromRomanNumeral", "::", "nrom"}], "]"}], ";", "\n", "    ", 
