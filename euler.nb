       RowBox[{"col", "\[Equal]", 
        RowBox[{"cols", "-", "1"}]}]}], ":=", "\n", "        ", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"index", "[", 
          RowBox[{"row", ",", "col"}], "]"}], ",", 
         RowBox[{"index", "[", 
          RowBox[{"row", ",", 
           RowBox[{"col", "+", "1"}]}], "]"}]}], "}"}], "\[Rule]", 
       RowBox[{"weight", "+", 
        RowBox[{"matrix", "\[LeftDoubleBracket]", 
         RowBox[{"row", ",", 
          RowBox[{"col", "+", "1"}]}], "\[RightDoubleBracket]"}]}]}]}], ";", 
     "\n", "    ", 
     RowBox[{
      RowBox[{"right", "[", 
       RowBox[{"weight_", ",", 
        RowBox[{"{", 
         RowBox[{"row_", ",", "col_"}], "}"}]}], "]"}], ":=", "\n", 
      "        ", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"index", "[", 
          RowBox[{"row", ",", "col"}], "]"}], ",", 
         RowBox[{"index", "[", 
          RowBox[{"row", ",", 
           RowBox[{"col", "+", "1"}]}], "]"}]}], "}"}], "\[Rule]", 
       "weight"}]}], ";", "\n", "        ", "\n", "    ", 
     RowBox[{
      RowBox[{"encode", "[", "x__", "]"}], ":=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"up", "[", "x", "]"}], ",", 
        RowBox[{"down", "[", "x", "]"}], ",", 
        RowBox[{"right", "[", "x", "]"}]}], "}"}]}], ";", "\n", "\n", "    ", 
     
     RowBox[{"g", "=", 
      RowBox[{"WeightedAdjacencyGraph", "@", 
       RowBox[{"SparseArray", "[", "\n", "            ", 
        RowBox[{
         RowBox[{"Flatten", "@", 
          RowBox[{"MapIndexed", "[", 
           RowBox[{"encode", ",", "matrix", ",", 
            RowBox[{"{", "2", "}"}]}], "]"}]}], ",", "\n", "            ", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"rows", "*", "cols"}], ",", 
           RowBox[{"rows", "*", "cols"}]}], "}"}], ",", "Infinity"}], 
        "]"}]}]}], ";", "\n", "\n", "    ", 
     RowBox[{"Min", "@", 
      RowBox[{"ParallelMap", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"GraphDistance", "[", 
          RowBox[{"g", ",", 
           RowBox[{"index", "[", 
            RowBox[{
             RowBox[{
             "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], ",",
              "1"}], "]"}], ",", 
           RowBox[{"index", "[", 
            RowBox[{
             RowBox[{
             "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], ",",
              "cols"}], "]"}]}], "]"}], "&"}], ",", 
        RowBox[{"Tuples", "[", 
         RowBox[{
          RowBox[{"Range", "[", "rows", "]"}], ",", 
          RowBox[{"{", "2", "}"}]}], "]"}]}], "]"}]}]}]}], "\n", "]"}], "//", 
  "AbsoluteTiming"}]], "Input",
