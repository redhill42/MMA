     RowBox[{
      RowBox[{"StringLength", "[", "roman", "]"}], "-", 
      RowBox[{"StringLength", "[", 
       RowBox[{"RomanNumeral", "@", 
        RowBox[{"FromRomanNumeral", "[", "roman", "]"}]}], "]"}]}]}], ";", 
    "\n", "    ", 
