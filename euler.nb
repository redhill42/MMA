Cell[TextData[ButtonBox["12 Highly divisible triangular number",
 BaseStyle->"Hyperlink",
 ButtonData->{
   URL["https://projecteuler.net/problem=12"], None},
 ButtonNote->"https://projecteuler.net/problem=12"]], "Subsubsection",
 CellChangeTimes->{{3.730262470527892*^9, 3.730262479238802*^9}, {
  3.7304955492939987`*^9, 3.7304955492988234`*^9}, {3.730626177406907*^9, 
  3.73062617786307*^9}},ExpressionUUID->"03448ab5-2b2e-4fbc-9a13-\
e132722234a5"],

Cell[TextData[{
 "The sequence of triangle numbers is generated by adding the natural \
numbers, So the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["7", "th"], TraditionalForm]],ExpressionUUID->
  "e4aa3dec-132f-44d8-a77b-359273c9ea6d"],
 " triangle number would be ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"1", "+", "2", "+", "3", "+", "4", "+", "5", "+", "6", "+", "7"}],
     "=", "28"}], TraditionalForm]],ExpressionUUID->
  "37e1a6ce-3468-4c23-9628-224bb9d65cf2"],
 ". The first ten terms would be:\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
   "1", ",", "3", ",", "6", ",", "10", ",", "15", ",", "21", ",", "28", ",", 
    "36", ",", "45", ",", "55", ",", "..."}], TraditionalForm]],
  ExpressionUUID->"a083eedc-55b2-401c-916e-65d6d9ea9e00"],
 "\nLet us list the factors of the first seven triangle numbers:\n\t\t  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"  ", 
    RowBox[{
     RowBox[{"1", ":", " ", "1"}], "\[IndentingNewLine]", "  ", 
     RowBox[{
      RowBox[{"3", ":", " ", "1"}], ",", "2", ",", "3", ",", "6"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"10", ":", " ", "1"}], ",", "2", ",", "5", ",", "10"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"15", ":", " ", "1"}], ",", "3", ",", "5", ",", "15"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"21", ":", " ", "1"}], ",", "3", ",", "7", ",", "21"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"28", ":", " ", "1"}], ",", "2", ",", "4", ",", "7", ",", "14", 
      ",", "28"}]}]}], TraditionalForm]],ExpressionUUID->
  "5888ee7e-bae9-40a0-9637-b131d5bae9ac"],
 "\nWe can see that 28 is the first triangle number to have over five \
divisors.\nWhat is the value of the first triangle number to have over five \
hundred divisors?"
}], "Text",
 CellChangeTimes->{{3.730262481719043*^9, 3.730262646583501*^9}, {
  3.7305074599975233`*^9, 
  3.7305074642765408`*^9}},ExpressionUUID->"4eb0a350-98a5-45df-a093-\
f18253a37c7d"],

