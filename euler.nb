Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]],ExpressionUUID->
  "75317f58-2db9-4623-9c25-d64323034d30"],
 " is the perimeter of a right angle triangle with integral length sides, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"a", ",", "b", ",", "c"}], "}"}], TraditionalForm]],
  ExpressionUUID->"a26453f7-2374-4158-b7ef-09d12a8fdc22"],
 ", there are exactly three solutions for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", " ", "=", " ", "120"}], TraditionalForm]],ExpressionUUID->
  "6d87370c-711e-4e4f-9903-4b3e3fe219da"],
 ".\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{"20", ",", "48", ",", "52"}], "}"}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"24", ",", "45", ",", "51"}], "}"}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"30", ",", "40", ",", "50"}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"4210f9bc-73c1-46a4-a36f-d98c388166e7"],
 "\nFor which value of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "\[LessEqual]", "1000"}], TraditionalForm]],ExpressionUUID->
  "eeb8068b-ee6a-4f00-856a-9d6656b7ad96"],
 ", is the number of solutions maximised?"
}], "Text",
