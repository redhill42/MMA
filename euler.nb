Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "(", "n", ")"}], TraditionalForm]],ExpressionUUID->
  "ab2f3e75-b7de-42a4-b3d9-a0f1d3a30773"],
 " represent the number of different ways in which ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "c63cf7a8-805c-40ba-91b9-33aa8dc1778f"],
 " coins can be separated into piles. For example, five coins can be \
separated into piles in exactly seven different ways, so ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"p", "(", "5", ")"}], "=", "7"}], TraditionalForm]],
  ExpressionUUID->"3e9a217e-7189-4bd4-b11d-69eb154e1047"],
 ".\n\t",
 Cell[BoxData[
  FormBox[GridBox[{
     {"ooooo"},
     {
      RowBox[{"ooooo", "    ", "o"}]},
     {
      RowBox[{"ooo", "    ", "oo"}]},
     {
      RowBox[{"ooo", "    ", "o", "    ", "o"}]},
     {
      RowBox[{"oo", "    ", "oo", "    ", "o"}]},
     {
      RowBox[{"oo", "    ", "oo", "    ", "o"}]},
     {
      RowBox[{"oo", "    ", "o", "     ", "o", "    ", "o"}]},
     {
      RowBox[{"o", "    ", "o", "    ", "o", "    ", "o", "    ", "o"}]}
    }], TraditionalForm]],ExpressionUUID->
  "86a50e9d-b905-4cb7-90c6-eef48504cbb4"],
 "\nFind the least value of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "c2dd274e-e43a-4bf0-9920-d5fa9239f6ad"],
 " for which ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "(", "n", ")"}], TraditionalForm]],ExpressionUUID->
  "bac92e78-5277-40d5-9338-1e11dafee6b5"],
 " is divisible by one million."
}], "Text",
 CellChangeTimes->{{3.730677606974299*^9, 3.730677794750785*^9}, {
  3.730685103807845*^9, 
  3.730685104156783*^9}},ExpressionUUID->"e193cc52-f83f-468d-a450-\
baf1ff69e819"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"p", ",", " ", "penta"}], "}"}], ",", "\n", "    ", 
    RowBox[{
     RowBox[{
      RowBox[{"p", "[", 
       RowBox[{"0", "|", "1"}], "]"}], "=", "1"}], ";", "\n", "    ", "\n", 
     "    ", 
     RowBox[{
      RowBox[{
       RowBox[{"penta", "[", 
        RowBox[{
        "n_", ",", "k_", ",", "a_", ",", "b_", ",", "s_", ",", "sum_"}], 
        "]"}], "/;", 
       RowBox[{"n", "\[GreaterEqual]", "a"}]}], ":=", "\n", "        ", 
      RowBox[{"penta", "[", 
       RowBox[{"n", ",", 
        RowBox[{"k", "+", "3"}], ",", 
        RowBox[{"a", "+", "k", "+", "1"}], ",", 
        RowBox[{"b", "+", "k"}], ",", 
        RowBox[{"-", "s"}], ",", 
        RowBox[{"sum", "+", 
         RowBox[{"s", "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"p", "[", 
             RowBox[{"n", "-", "a"}], "]"}], "+", 
            RowBox[{"p", "[", 
             RowBox[{"n", "-", "b"}], "]"}]}], ")"}]}]}]}], "]"}]}], ";", 
     "\n", "    ", 
     RowBox[{
      RowBox[{"penta", "[", 
       RowBox[{
       "n_", ",", "k_", ",", "a_", ",", "b_", ",", "s_", ",", "sum_"}], "]"}],
       ":=", "\n", "       ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"n", "\[GreaterEqual]", "b"}], ",", 
        RowBox[{"sum", "+", 
         RowBox[{"s", "*", 
          RowBox[{"p", "[", 
           RowBox[{"n", "-", "b"}], "]"}]}]}], ",", "sum"}], "]"}]}], ";", 
     "\n", "    ", 
     RowBox[{
      RowBox[{"penta", "[", "n_", "]"}], ":=", 
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{"penta", "[", 
         RowBox[{"n", ",", "4", ",", "2", ",", "1", ",", "1", ",", "0"}], 
         "]"}], ",", "1000000"}], "]"}]}], ";", "\n", "    ", "\n", "    ", 
     RowBox[{"NestWhile", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"#", "+", "1"}], "&"}], ",", "2", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"p", "[", "#", "]"}], "=", 
           RowBox[{"penta", "[", "#", "]"}]}], ")"}], "\[NotEqual]", "0"}], 
        "&"}]}], "]"}]}]}], "\n", "]"}], " ", "//", " ", 
  "AbsoluteTiming"}]], "Input",
