Cell[TextData[{
 StyleBox["import",
  FontColor->RGBColor[0, 0, 1]],
 " java.util.ArrayList;\n",
 StyleBox["import",
  FontColor->RGBColor[0, 0, 1]],
 " java.util.Arrays;\n",
 StyleBox["import",
  FontColor->RGBColor[0, 0, 1]],
 " java.util.BitSet;\n",
 StyleBox["import",
  FontColor->RGBColor[0, 0, 1]],
 " java.util.HashMap;\n",
 StyleBox["import",
  FontColor->RGBColor[0, 0, 1]],
 " java.util.List;\n",
 StyleBox["import",
  FontColor->RGBColor[0, 0, 1]],
 " java.util.Map;\n\n",
 StyleBox["public class",
  FontColor->RGBColor[0, 0, 1]],
 " PrimeSet {\n    ",
 StyleBox["static final int",
  FontColor->RGBColor[0, 0, 1]],
 " MAX = 98765432;\n\n    BitSet primes = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " BitSet(MAX);\n    Map<Integer,Integer> candidates = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " HashMap<>();\n\n    @SuppressWarnings(\"unchecked\")\n    List<Integer>[] \
candidateGroups = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " List[9];\n\n    {\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 1; i < candidateGroups.length; i++) {\n            candidateGroups[i] \
= ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " ArrayList<>();\n        }\n    }\n\n    ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[][] partitions = {\n        {1, 8},\n        {2, 7},\n        {1, 1, 7},\n \
       {3, 6},\n        {1, 2, 6},\n        {1, 1, 1, 6},\n        {4, 5},\n  \
      {1, 3, 5},\n        {2, 2, 5},\n        {1, 1, 2, 5},\n        {1, 1, \
1, 1, 5},\n        {1, 4, 4},\n        {2, 3, 4},\n        {1, 1, 3, 4},\n    \
    {1, 2, 2, 4},\n        {1, 1, 1, 2, 4},\n        {3, 3, 3},\n        {1, \
2, 3, 3},\n        {1, 1, 1, 3, 3},\n        {2, 2, 2, 3},\n        {1, 1, 2, \
2, 3},\n        {1, 1, 1, 1, 2, 3},\n        {1, 2, 2, 2, 2},\n        {1, 1, \
1, 2, 2, 2}\n    };\n\n    ",
 StyleBox["void",
  FontColor->RGBColor[0, 0, 1]],
 " sieve() {\n        primes.set(2);\n        addCandidate(2);\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 3; i < MAX; i += 2) {\n            primes.set(i);\n        }\n\n       \
 ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " sq = (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 ")Math.sqrt(MAX);\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 3; i <= sq; i += 2) {\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (primes.get(i)) {\n                addCandidate(i);\n                ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " j = i*i; j < MAX; j += i)\n                    primes.clear(j);\n          \
  }\n        }\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = ((sq+1)&~1)+1; i < MAX; i += 2) {\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (primes.get(i))\n                addCandidate(i);\n        }\n    }\n\n    \
",
 StyleBox["private void",
  FontColor->RGBColor[0, 0, 1]],
 " addCandidate(",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " prime) {\n        ",
 StyleBox["char",
  FontColor->RGBColor[0, 0, 1]],
 "[] digits = Integer.toString(prime).toCharArray();\n        \
Arrays.sort(digits);\n        ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (!isPandigital(digits))\n            ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 ";\n        prime = Integer.parseInt(",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " String(digits));\n\n        Integer count = candidates.get(prime);\n       \
 ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (count == ",
 StyleBox["null",
  FontColor->RGBColor[0, 0, 1]],
 ") {\n            count = 1;\n            \
candidateGroups[digits.length].add(prime);\n        } ",
 StyleBox["else",
  FontColor->RGBColor[0, 0, 1]],
 " {\n            count++;\n        }\n        candidates.put(prime, count);\n\
    }\n\n    ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " solve() {\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " total = 0;\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[] set = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[10];\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[] group : partitions) {\n            total += countPrimeSets(group, 0, \
set);\n        }\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " total;\n    }\n\n    ",
 StyleBox["private",
  FontColor->RGBColor[0, 0, 1]],
 " ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " countPrimeSets(",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[] group, ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i, ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[] set) {\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " total = 0;\n\n        ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (i == group.length) {\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (isPandigitalSet(set, i)) {\n                total = 1;\n                \
",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " j = 0; j < i; j++) {\n                    total *= candidates.get(set[j]);\
\n                }\n            }\n        } ",
 StyleBox["else",
  FontColor->RGBColor[0, 0, 1]],
 " {\n            ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " prime : candidateGroups[group[i]]) {\n                ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (i == 0 || prime > set[i-1]) {\n                    set[i] = prime;\n      \
              total += countPrimeSets(group, i+1, set);\n                }\n  \
          }\n        }\n\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " total;\n    }\n\n    ",
 StyleBox["private static boolean",
  FontColor->RGBColor[0, 0, 1]],
 " isPandigital(",
 StyleBox["char",
  FontColor->RGBColor[0, 0, 1]],
 "[] digits) {\n        ",
 StyleBox["char",
  FontColor->RGBColor[0, 0, 1]],
 " c = 0;\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["char",
  FontColor->RGBColor[0, 0, 1]],
 " d : digits) {\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (d == '0')\n                ",
 StyleBox["return false",
  FontColor->RGBColor[0, 0, 1]],
 ";\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (d == c)\n                ",
 StyleBox["return false",
  FontColor->RGBColor[0, 0, 1]],
 ";\n            c = d;\n        }\n        ",
 StyleBox["return true",
  FontColor->RGBColor[0, 0, 1]],
 ";\n    }\n\n    ",
 StyleBox["private static boolean",
  FontColor->RGBColor[0, 0, 1]],
 " isPandigitalSet(",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[] set, ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " len) {\n        StringBuilder buf = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " StringBuilder();\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 0; i < len; i++) {\n            buf.append(set[i]);\n        }\n\n     \
   ",
 StyleBox["char",
  FontColor->RGBColor[0, 0, 1]],
 "[] digits = buf.toString().toCharArray();\n        Arrays.sort(digits);\n   \
     ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " isPandigital(digits);\n    }\n\n    ",
 StyleBox["public static void",
  FontColor->RGBColor[0, 0, 1]],
 " main(String[] args) {\n        PrimeSet solver = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " PrimeSet();\n        solver.sieve();\n        \
System.out.println(solver.solve());\n    }\n}"
}], "Program",
