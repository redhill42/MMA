      "\n", "    ", 
     RowBox[{
      RowBox[{"unindex", "[", "i_", "]"}], ":=", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"i", "-", "1"}], ")"}], "~", "QuotientRemainder", "~", 
        "cols"}], "+", 
       RowBox[{"{", 
        RowBox[{"1", ",", "1"}], "}"}]}]}], ";", "\n", "\n", "    ", 
     RowBox[{
      RowBox[{"up", "[", 
