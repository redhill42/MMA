Cell[TextData[{
 StyleBox["public class",
  FontColor->RGBColor[0, 0, 1]],
 " CoinPartitions {\n\t",
 StyleBox["private int",
  FontColor->RGBColor[0, 0, 1]],
 " p[] = ",
 StyleBox["new int",
  FontColor->RGBColor[0, 0, 1]],
 "[1000000];\n\n\t",
 StyleBox["private int",
  FontColor->RGBColor[0, 0, 1]],
 " partition(",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " n) {\n\t\t",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " k = 4;\n\t\t",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " a = 2;\n\t\t",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " b = 1;\n\t\t",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " s = 1;\n\t\t",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " sum = 0;\n\t\t\n\t\t",
 StyleBox["while",
  FontColor->RGBColor[0, 0, 1]],
 " (n >= a) {\n\t   \t\tsum += s * (p[n - a] + p[n - b]);\n\t\t\ta += k + 1;\n\
\t\t\tb += k;\n\t\t\tk += 3;\n\t\t\ts = -s;\n\t\t}\n\t\t",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (n >= b) {\n\t\t\tsum += s * p[n - b];\n\t\t}\n\t\t",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " p[n] = sum % 1000000;\n    }\n\n\t",
 StyleBox["public int",
  FontColor->RGBColor[0, 0, 1]],
 " findParition() {\n\t\tp[0] = p[1] = 1;\n\t\t",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " n = 2; ; n++) {\n\t\t\t",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (partition (n) == 0) {\n\t\t\t\t",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " n;\n\t\t\t}\n\t\t}\n\t}\n\t\n\t",
 StyleBox["public static void",
  FontColor->RGBColor[0, 0, 1]],
 " main(String[] args) {\n\t\tCoinPartition solver = ",
 StyleBox["new",
  FontColor->RGBColor[0, 0, 1]],
 " CoinPartiton();\n\t\tSystem.out.println(solver.findPartition());\n\t}\n}"
}], "Program",
 CellChangeTimes->{{3.7306858035890303`*^9, 3.7306860621746883`*^9}, {
  3.730686115365254*^9, 
  3.730686406268897*^9}},ExpressionUUID->"b03c9876-b7bb-4c5a-80d9-\
80a25e195d51"]
}, Open  ]],

