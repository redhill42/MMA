     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Mod", "[", 
         RowBox[{"num", ",", "100"}], "]"}], "\[Equal]", 
        RowBox[{"Floor", "[", 
         RowBox[{
          RowBox[{"First", "[", "chain", "]"}], "/", "100"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"Append", "[", 
         RowBox[{"chain", ",", "num"}], "]"}], "}"}], ",", "Nothing"}], 
      "]"}]}], ";", "\n", "        ", "\n", "    ", 
    RowBox[{
     RowBox[{"cyclic", "[", 
      RowBox[{"num_", ",", "polygonals_", ",", "chain_"}], "]"}], ":=", "\n", 
