          RowBox[{"8", "x"}], "+", "1"}]], "-", "1"}], "2"], "]"}]}], ";", 
    "\n", "    ", 
    RowBox[{
     RowBox[{
      RowBox[{"polygonalQ", "[", "4", "]"}], "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
