Cell[TextData[{
 StyleBox["package",
  FontColor->RGBColor[0, 0, 1]],
 " euler;\n\n",
 StyleBox["public class",
  FontColor->RGBColor[0, 0, 1]],
 " Problem154 {\n    ",
 StyleBox["private static final int",
  FontColor->RGBColor[0, 0, 1]],
 " LAYERS = 200000;\n    ",
 StyleBox["private static final int",
  FontColor->RGBColor[0, 0, 1]],
 " PRIME1 = 2;\n    ",
 StyleBox["private static final int",
  FontColor->RGBColor[0, 0, 1]],
 " EXPONENT1 = 12;\n    ",
 StyleBox["private static final int",
  FontColor->RGBColor[0, 0, 1]],
 " PRIME2 = 5;\n    ",
 StyleBox["private static final int",
  FontColor->RGBColor[0, 0, 1]],
 " EXPONENT2 = 12;\n\n    ",
 StyleBox["private final int",
  FontColor->RGBColor[0, 0, 1]],
 "[] sum1 = ",
 StyleBox["new int",
  FontColor->RGBColor[0, 0, 1]],
 "[LAYERS + 2];\n    ",
 StyleBox["private final int",
  FontColor->RGBColor[0, 0, 1]],
 "[] sum2 = ",
 StyleBox["new int",
  FontColor->RGBColor[0, 0, 1]],
 "[LAYERS + 2];\n\n    ",
 StyleBox["public",
  FontColor->RGBColor[0, 0, 1]],
 " Problem154() {\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " count1 = 0, count2 = 0;\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 1; i <= LAYERS; i++) {\n            ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " current = i;\n\n            ",
 StyleBox["while",
  FontColor->RGBColor[0, 0, 1]],
 " (current % PRIME1 == 0) {\n                current /= PRIME1;\n            \
    count1++;\n            }\n            sum1[i] = count1;\n\n            ",
 StyleBox["while",
  FontColor->RGBColor[0, 0, 1]],
 " (current % PRIME2 == 0) {\n                current /= PRIME2;\n            \
    count2++;\n            }\n            sum2[i] = count2;\n        }\n    }\
\n\n    ",
 StyleBox["private static int",
  FontColor->RGBColor[0, 0, 1]],
 " choose(",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[] sums, ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " n, ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " k) {\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " sums[n] - (sums[n - k] + sums[k]);\n    }\n\n    ",
 StyleBox["public long",
  FontColor->RGBColor[0, 0, 1]],
 " solve() {\n        ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " result = 0;\n\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " i = 0; i <= LAYERS; i++) {\n            ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " s1 = choose(sum1, LAYERS, i);\n            ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " s2 = choose(sum2, LAYERS, i);\n\n            ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (s1 >= EXPONENT1 && s2 >= EXPONENT2) {\n                result += i + 1;\n \
               ",
 StyleBox["continue",
  FontColor->RGBColor[0, 0, 1]],
 ";\n            }\n\n            ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " j = 0; j <= (i+1)/2; j++) {\n                ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (s1 + choose(sum1, i, j) >= EXPONENT1 &&\n                    s2 + \
choose(sum2, i, j) >= EXPONENT2) {\n                    result++;\n           \
         ",
 StyleBox["if",
  FontColor->RGBColor[0, 0, 1]],
 " (j < i / 2)\n                        result++;\n                }\n        \
    }\n        }\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " result;\n    }\n\n    ",
 StyleBox["public static void",
  FontColor->RGBColor[0, 0, 1]],
 " main(String[] args) {\n        System.out.println(new \
Problem154().solve());\n    }\n}"
}], "Program",ExpressionUUID->"28fb2574-dbfd-429f-9e11-111ea5df61fb"]
}, Open  ]],

