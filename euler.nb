Cell[TextData[{
 "In the 5 by 5 matrix below, the minimal path sum from the top left to the \
bottom right, by ",
 StyleBox["only moving to right and down",
  FontWeight->"Bold"],
 ", is indicated in bold red and is equal to 2427.\n\t\t\t",
 Cell[BoxData[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      StyleBox["131",
       FontColor->RGBColor[1, 0, 0]], "673", "234", "103", "18"},
     {
      StyleBox["201",
       FontColor->RGBColor[1, 0, 0]], 
      StyleBox["96",
       FontColor->RGBColor[1, 0, 0]], 
      StyleBox["342",
       FontColor->RGBColor[1, 0, 0]], "954", "150"},
     {"630", "803", 
      StyleBox["746",
       FontColor->RGBColor[1, 0, 0]], 
      StyleBox["422",
       FontColor->RGBColor[1, 0, 0]], "111"},
     {"537", "699", "497", 
      StyleBox["121",
       FontColor->RGBColor[1, 0, 0]], "956"},
     {"805", "732", "524", 
      StyleBox["37",
       FontColor->RGBColor[1, 0, 0]], 
      StyleBox["331",
       FontColor->RGBColor[1, 0, 0]]}
    }], "\[NoBreak]", ")"}]],ExpressionUUID->
  "9d0e093d-30e8-4ea9-8f1e-9219ec0e4b6a"],
 "\nFind the minimal path sum, in ",
 ButtonBox["matrix.txt",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://projecteuler.net/project/resources/p081_matrix.txt"], None},
  ButtonNote->"https://projecteuler.net/project/resources/p081_matrix.txt"],
 ", a 31K text file containing a 80 by 80 matrix, from the top left to the \
bottom right by only moving right and down."
}], "Text",
 CellChangeTimes->{{3.730528578397629*^9, 3.7305286818048973`*^9}, {
  3.730528748533016*^9, 
  3.730528792627554*^9}},ExpressionUUID->"647d8481-165c-4db4-90b6-\
6478336d30f2"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"matrix", ",", " ", "dim", ",", " ", "route"}], "}"}], ",", "\n", 
   "    ", 
   RowBox[{
    RowBox[{"matrix", "=", 
     RowBox[{"Import", "[", 
      RowBox[{
      "\"\<https://projecteuler.net/project/resources/p081_matrix.txt\>\"", 
       ",", "\"\<CSV\>\""}], "]"}]}], ";", "\n", "    ", 
    RowBox[{"dim", "=", 
     RowBox[{"Dimensions", "[", "matrix", "]"}]}], ";", "\n", "    ", "\n", 
    "    ", 
    RowBox[{
     RowBox[{"route", "[", 
      RowBox[{"row_", ",", "col_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"route", "[", 
       RowBox[{"row", ",", "col"}], "]"}], "=", "\n", "        ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"row", ",", "col"}], "}"}], "\[Equal]", "dim"}], ",", "\n", 
        "            ", 
        RowBox[{"m", "\[LeftDoubleBracket]", 
         RowBox[{"row", ",", "col"}], "\[RightDoubleBracket]"}], ",", "\n", 
        "            ", 
        RowBox[{"With", "[", 
         RowBox[{
          RowBox[{"{", "\n", "                ", 
           RowBox[{
            RowBox[{"down", "=", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"row", "<", 
                RowBox[{
                "dim", "\[LeftDoubleBracket]", "1", 
                 "\[RightDoubleBracket]"}]}], ",", 
               RowBox[{"route", "[", 
                RowBox[{
                 RowBox[{"row", "+", "1"}], ",", "col"}], "]"}], ",", 
               "Infinity"}], "]"}]}], ",", "\n", "                ", 
            RowBox[{"right", "=", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"col", "<", 
                RowBox[{
                "dim", "\[LeftDoubleBracket]", "2", 
                 "\[RightDoubleBracket]"}]}], ",", 
               RowBox[{"route", "[", 
                RowBox[{"row", ",", 
                 RowBox[{"col", "+", "1"}]}], "]"}], ",", "Infinity"}], 
              "]"}]}]}], "\n", "            ", "}"}], ",", " ", 
          RowBox[{
           RowBox[{"m", "\[LeftDoubleBracket]", 
            RowBox[{"row", ",", "col"}], "\[RightDoubleBracket]"}], "+", 
           RowBox[{"Min", "[", 
            RowBox[{"down", ",", "right"}], "]"}]}]}], "]"}]}], "]"}]}]}], 
    ";", "\n", "    ", 
    RowBox[{"route", "[", 
     RowBox[{"1", ",", "1"}], "]"}]}]}], "\n", "]"}]], "Input",
