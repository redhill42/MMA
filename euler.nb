    RowBox[{
     SuperscriptBox["3", "3"], "=", "27"}], ",", " ", 
    RowBox[{
     SuperscriptBox["3", "4"], "=", "81"}], ",", " ", 
    RowBox[{
     SuperscriptBox["3", "5"], "=", "243"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["4", "2"], "=", "16"}], ",", " ", 
    RowBox[{
     SuperscriptBox["4", "3"], "=", "64"}], ",", " ", 
    RowBox[{
     SuperscriptBox["4", "4"], "=", "256"}], ",", " ", 
