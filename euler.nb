 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"matrix", ",", " ", "dim", ",", " ", "route"}], "}"}], ",", "\n", 
   "    ", 
   RowBox[{
    RowBox[{"matrix", "=", 
     RowBox[{"Import", "[", 
      RowBox[{
      "\"\<https://projecteuler.net/project/resources/p081_matrix.txt\>\"", 
       ",", "\"\<CSV\>\""}], "]"}]}], ";", "\n", "    ", 
    RowBox[{"dim", "=", 
     RowBox[{"Dimensions", "[", "matrix", "]"}]}], ";", "\n", "    ", "\n", 
    "    ", 
    RowBox[{
     RowBox[{"route", "[", 
      RowBox[{"row_", ",", "col_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"route", "[", 
       RowBox[{"row", ",", "col"}], "]"}], "=", "\n", "        ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"row", ",", "col"}], "}"}], "\[Equal]", "dim"}], ",", "\n", 
        "            ", 
        RowBox[{"m", "\[LeftDoubleBracket]", 
         RowBox[{"row", ",", "col"}], "\[RightDoubleBracket]"}], ",", "\n", 
        "            ", 
        RowBox[{"With", "[", 
         RowBox[{
          RowBox[{"{", "\n", "                ", 
           RowBox[{
            RowBox[{"down", "=", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"row", "<", 
                RowBox[{
                "dim", "\[LeftDoubleBracket]", "1", 
                 "\[RightDoubleBracket]"}]}], ",", 
               RowBox[{"route", "[", 
                RowBox[{
                 RowBox[{"row", "+", "1"}], ",", "col"}], "]"}], ",", 
               "Infinity"}], "]"}]}], ",", "\n", "                ", 
            RowBox[{"right", "=", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"col", "<", 
                RowBox[{
                "dim", "\[LeftDoubleBracket]", "2", 
                 "\[RightDoubleBracket]"}]}], ",", 
               RowBox[{"route", "[", 
                RowBox[{"row", ",", 
                 RowBox[{"col", "+", "1"}]}], "]"}], ",", "Infinity"}], 
              "]"}]}]}], "\n", "            ", "}"}], ",", " ", 
          RowBox[{
           RowBox[{"m", "\[LeftDoubleBracket]", 
            RowBox[{"row", ",", "col"}], "\[RightDoubleBracket]"}], "+", 
           RowBox[{"Min", "[", 
            RowBox[{"down", ",", "right"}], "]"}]}]}], "]"}]}], "]"}]}]}], 
    ";", "\n", "    ", 
    RowBox[{"route", "[", 
     RowBox[{"1", ",", "1"}], "]"}]}]}], "\n", "]"}]], "Input",
