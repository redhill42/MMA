Cell[CellGroupData[{

Cell[TextData[ButtonBox["43 Sub-string divisibility",
 BaseStyle->"Hyperlink",
 ButtonData->{
   URL["https://projecteuler.net/problem=43"], None},
 ButtonNote->"https://projecteuler.net/problem=43"]], "Subsubsection",
 CellChangeTimes->{{3.730346537192154*^9, 3.730346542911777*^9}, {
  3.7304975932508783`*^9, 3.7304975932557898`*^9}, {3.7306263025348454`*^9, 
  3.73062630293504*^9}},ExpressionUUID->"079a33ed-eb26-497c-8b57-\
b20d8895fd03"],

Cell[TextData[{
 "The number, 1406357289, is a 0 to 9 pandigital number because it is made up \
of each of the digits 0 to 9 in some order, but it also has a rather \
interesting sub-string divisibility property.\nLet ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5efb67e4-e443-4c6a-8173-69e55ee897c2"],
 " be the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["1", "st"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a8d92a77-3823-43c9-b03f-739a1ebaed02"],
 " digit, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bc4e8453-9138-4336-b99d-f10e40cd89b5"],
 " be the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["2", "nd"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0c0061d4-40bb-47bd-a944-27fafa6b2a23"],
 " digit, and so on. In this way, we note the following:\n\t\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["d", "2"], 
     SubscriptBox["d", "3"], 
     SubscriptBox["d", "4"]}], "=", 
    RowBox[{"406", " ", "is", " ", "divisible", " ", "by", " ", "2"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["d", "3"], 
     SubscriptBox["d", "4"], 
     SubscriptBox["d", "5"]}], "=", 
    RowBox[{"063", " ", "is", " ", "divisible", " ", "by", " ", "3"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["d", "4"], 
     SubscriptBox["d", "5"], 
     SubscriptBox["d", "6"]}], "=", 
    RowBox[{"635", " ", "is", " ", "divisible", " ", "by", " ", "5"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["d", "5"], 
     SubscriptBox["d", "6"], 
     SubscriptBox["d", "7"]}], "=", 
    RowBox[{"357", " ", "is", " ", "divisible", " ", "by", " ", "7"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["d", "6"], 
     SubscriptBox["d", "7"], 
     SubscriptBox["d", "8"]}], "=", 
    RowBox[{"572", " ", "is", " ", "divisible", " ", "by", " ", "11"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["d", "7"], 
     SubscriptBox["d", "8"], 
     SubscriptBox["d", "9"]}], "=", 
    RowBox[{"728", " ", "is", " ", "divisible", " ", "by", " ", "13"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["d", "8"], 
     SubscriptBox["d", "9"], 
     SubscriptBox["d", "10"]}], "=", 
    RowBox[{"289", " ", "is", " ", "divisible", " ", "by", " ", "17"}]}], 
   TraditionalForm]}],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d8719e4a-a037-4933-ae93-8a1f5ebbbd13"],
 "\nFind the sum of all 0 to 9 pandigital numbers with this property."
}], "Text",
 CellChangeTimes->{{3.730346546352017*^9, 
  3.7303467348803043`*^9}},ExpressionUUID->"6553b790-cb7d-4bd3-827b-\
85cc1defd09a"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", "filterDivisor", "}"}], ",", "\n", "    ", 
   RowBox[{
    RowBox[{
     RowBox[{"filterDivisor", "[", 
      RowBox[{"digits_", ",", "divisor_"}], "]"}], ":=", "\n", "        ", 
     RowBox[{"Select", "[", "\n", "            ", 
      RowBox[{
       RowBox[{"Catenate", "@", 
        RowBox[{"Outer", "[", 
         RowBox[{"Prepend", ",", "digits", ",", 
          RowBox[{"Range", "[", 
           RowBox[{"0", ",", "9"}], "]"}], ",", "1"}], "]"}]}], ",", "\n", 
       "            ", 
       RowBox[{
        RowBox[{
         RowBox[{"DuplicateFreeQ", "[", "#", "]"}], "&&", 
         RowBox[{
          RowBox[{"FromDigits", "@", 
           RowBox[{"Take", "[", 
            RowBox[{"#", ",", "3"}], "]"}]}], "~", "Divisible", "~", 
          "divisor"}]}], "&"}]}], "\n", "        ", "]"}]}], ";", "\n", "\n", 
    "    ", 
    RowBox[{"Total", "@", 
     RowBox[{
      RowBox[{"Map", "[", "FromDigits", "]"}], "@", "\n", "        ", 
      RowBox[{"Fold", "[", 
       RowBox[{"filterDivisor", ",", 
        RowBox[{"Permutations", "[", 
         RowBox[{
          RowBox[{"Range", "[", 
           RowBox[{"0", ",", "9"}], "]"}], ",", 
          RowBox[{"{", "2", "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{
         "17", ",", "13", ",", "11", ",", "7", ",", "5", ",", "3", ",", "2", 
          ",", "1"}], "}"}]}], "]"}]}]}]}]}], "\n", "]"}]], "Input",
 CellChangeTimes->{{3.730346866105002*^9, 3.730347090056122*^9}, {
  3.730347136232445*^9, 3.730347152024291*^9}, {3.7303472467854233`*^9, 
  3.7303473120642*^9}, {3.7303475295376177`*^9, 3.730347531480298*^9}, {
  3.730347663481393*^9, 3.730347747736041*^9}, {3.730347809434209*^9, 
  3.7303478328638887`*^9}, {3.7303479471889553`*^9, 3.730348005866136*^9}, {
  3.730514187493834*^9, 3.730514319972886*^9}, {3.7305178634693213`*^9, 
  3.7305178790922737`*^9}},ExpressionUUID->"28b29177-9ced-487d-b70c-\
ff768283273e"],

