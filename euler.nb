Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"cipher", ",", "keys", ",", "decryption"}], "}"}], ",", "\n", 
   "    ", 
   RowBox[{
    RowBox[{"cipher", "=", 
     RowBox[{"First", "@", 
      RowBox[{"Import", "[", 
       RowBox[{
       "\"\<https://projecteuler.net/project/resources/p059_cipher.txt\>\"", 
        ",", " ", "\"\<CSV\>\""}], "]"}]}]}], ";", "\n", "    ", 
    RowBox[{"keys", "=", 
     RowBox[{"Tuples", "[", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{"ToCharacterCode", ",", 
         RowBox[{"CharacterRange", "[", 
          RowBox[{"\"\<a\>\"", ",", "\"\<z\>\""}], "]"}]}], "]"}], ",", "3"}],
       "]"}]}], ";", "\n", "  ", "\n", "    ", 
    RowBox[{
     RowBox[{"decryption", "[", 
      RowBox[{"message_", ",", "key_"}], "]"}], ":=", "\n", "        ", 
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"msglen", "=", 
          RowBox[{"Length", "[", "message", "]"}]}], ",", 
         RowBox[{"keylen", "=", 
          RowBox[{"Length", "[", "key", "]"}]}]}], "}"}], ",", "\n", 
       "            ", 
       RowBox[{"Take", "[", "\n", "                ", 
        RowBox[{
         RowBox[{"Flatten", "@", 
          RowBox[{"Map", "[", "\n", "                    ", 
           RowBox[{
            RowBox[{
             RowBox[{"BitXor", "[", 
              RowBox[{"key", ",", "#"}], "]"}], "&"}], ",", "\n", 
            "                    ", 
            RowBox[{"Partition", "[", 
             RowBox[{
              RowBox[{"PadRight", "[", 
               RowBox[{"message", ",", 
                RowBox[{
                 RowBox[{"Ceiling", "[", 
                  RowBox[{"msglen", "/", "keylen"}], "]"}], "*", "keylen"}]}],
                "]"}], ",", "keylen"}], "]"}]}], "\n", "            ", 
           "]"}]}], ",", "msglen"}], "]"}]}], "]"}]}], ";", "\n", "  ", "\n", 
    "    ", 
    RowBox[{"Total", "@", 
     RowBox[{"Flatten", "@", 
      RowBox[{"Select", "[", "\n", "        ", 
       RowBox[{
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"decryption", "[", 
            RowBox[{"cipher", ",", "#"}], "]"}], "&"}], ",", "keys"}], "]"}], 
        ",", "\n", "        ", 
        RowBox[{
         RowBox[{"StringContainsQ", "[", 
          RowBox[{
           RowBox[{"FromCharacterCode", "[", "#", "]"}], ",", 
           "\"\< was \>\""}], "]"}], "&"}]}], "]"}]}]}]}]}], "\n", 
  "]"}]], "Input",
