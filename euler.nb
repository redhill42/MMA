Cell[BoxData[{
 RowBox[{
  RowBox[{"solution", "[", "d_", "]"}], ":=", 
  RowBox[{"0", "/;", 
   RowBox[{"IntegerQ", "[", 
    RowBox[{"Sqrt", "[", "d", "]"}], "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"solution", "[", "d_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x", ",", "y"}], "}"}], ",", "\n", "    ", 
     RowBox[{"Cases", "[", "\n", "        ", 
      RowBox[{
       RowBox[{
        RowBox[{"First", "@", 
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"x", "^", "2"}], "-", 
              RowBox[{"d", " ", 
               RowBox[{"y", "^", "2"}]}]}], "\[Equal]", "1"}], "&&", 
            RowBox[{"x", ">", "0"}], "&&", 
            RowBox[{"y", ">", "0"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}], ",", "Integers"}], "]"}]}], "/.", 
        
        RowBox[{
         RowBox[{"C", "[", "1", "]"}], "\[Rule]", "1"}]}], ",", "\n", 
       "        ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "\[Rule]", "ans_"}], ")"}], "\[RuleDelayed]", "ans"}]}],
       "\n", "    ", "]"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"MaximalBy", "[", 
   RowBox[{
    RowBox[{"Range", "[", "1000", "]"}], ",", "solution"}], "]"}], "//", 
  "AbsoluteTiming"}]}], "Input",
