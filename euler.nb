          RowBox[{"24", "x"}], "+", "1"}]], "+", "1"}], "6"], "]"}]}], ";", 
    "\n", "    ", 
    RowBox[{
     RowBox[{
      RowBox[{"polygonalQ", "[", "6", "]"}], "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
      FractionBox[
       RowBox[{
