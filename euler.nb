Cell[TextData[ButtonBox["Diophantine equation",
 BaseStyle->"Hyperlink",
 ButtonData->{
   URL["https://projecteuler.net/problem=66"], None},
 ButtonNote->"https://projecteuler.net/problem=66"]], "Subsubsection",
 CellChangeTimes->{{3.730459750083695*^9, 3.730459754794303*^9}, {
  3.730497855077036*^9, 
  3.7304978550854187`*^9}},ExpressionUUID->"9ae90754-0f5c-4229-b24e-\
2f2cee95cfc0"],

Cell[TextData[{
 "Consider quadratic Diophantine equations of the form\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", 
      RowBox[{"2", " "}]], "-", 
     SuperscriptBox["Dy", "2"]}], "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cc02740a-7092-486a-932c-3d34659fa272"],
 "\nFor example, when ",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "=", "13"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0f8bd11d-b32c-4232-993d-481e132d5faf"],
 ", The minimal solution in x is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["649", "2"], "-", 
     RowBox[{"13", "\[Times]", 
      SuperscriptBox["180", "2"]}]}], "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d2690189-0515-477c-b8dc-c9a705a0ea87"],
 ".\nIt can be assumed that there are no solutions in positive integers when \
D is square.\nBy finding minimal solutions in x for D = {2, 3, 5, 6, 7}, we \
obtain the following:\n\t\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["3", "2"], "-", 
     RowBox[{"2", "\[Times]", 
      SuperscriptBox["2", "2"]}]}], "=", "1"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["2", "2"], "-", 
     RowBox[{"3", "\[Times]", 
      SuperscriptBox["1", "2"]}]}], "=", "1"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["9", "2"], "-", 
     RowBox[{"5", "\[Times]", 
      SuperscriptBox["4", "2"]}]}], "=", "1"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["5", "2"], "-", 
     RowBox[{"6", "\[Times]", 
      SuperscriptBox["2", "2"]}]}], "=", "1"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["8", "2"], "-", 
     RowBox[{"7", "\[Times]", 
      SuperscriptBox["3", "2"]}]}], "=", "1"}], TraditionalForm]}],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4f5cdb84-68ad-4ce5-bdda-5fcab710c4ce"],
 "\nHence, by considering minimal solutions in x for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "\[LessEqual]", "7"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "891d823e-5016-4e37-b848-6028a6da418c"],
 ", the largest ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3bd54bd7-cf9d-4c80-b966-9c15180d21d5"],
 " is obtained when ",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "=", "5"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "157e2b64-fee7-4c5f-907c-3fa0bddc474a"],
 ".\nFind the value of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "\[LessEqual]", "1000"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2eac5485-6fc0-4f61-901a-44bbb03dd7eb"],
 " in minimal solutions of x for which the largest value of x is obtained."
}], "Text",
 CellChangeTimes->{{3.730459757730802*^9, 
  3.730459956122982*^9}},ExpressionUUID->"1c96a7ab-74f2-4abf-bb90-\
35ae7b6b2282"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"solution", "[", "d_", "]"}], ":=", 
  RowBox[{"0", "/;", 
   RowBox[{"IntegerQ", "[", 
    RowBox[{"Sqrt", "[", "d", "]"}], "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"solution", "[", "d_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x", ",", "y"}], "}"}], ",", "\n", "    ", 
     RowBox[{"Cases", "[", "\n", "        ", 
      RowBox[{
       RowBox[{
        RowBox[{"First", "@", 
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"x", "^", "2"}], "-", 
              RowBox[{"d", " ", 
               RowBox[{"y", "^", "2"}]}]}], "\[Equal]", "1"}], "&&", 
            RowBox[{"x", ">", "0"}], "&&", 
            RowBox[{"y", ">", "0"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}], ",", "Integers"}], "]"}]}], "/.", 
        
        RowBox[{
         RowBox[{"C", "[", "1", "]"}], "\[Rule]", "1"}]}], ",", "\n", 
       "        ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "\[Rule]", "ans_"}], ")"}], "\[RuleDelayed]", "ans"}]}],
       "\n", "    ", "]"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"MaximalBy", "[", 
   RowBox[{
    RowBox[{"Range", "[", "1000", "]"}], ",", "solution"}], "]"}], "//", 
  "AbsoluteTiming"}]}], "Input",
 CellChangeTimes->{{3.730460007364313*^9, 3.7304600638038816`*^9}, {
  3.730461547830255*^9, 3.730461800572249*^9}, {3.730461840605842*^9, 
  3.730461841412552*^9}, {3.730464791362764*^9, 3.7304648428810997`*^9}, {
  3.730516937492383*^9, 
  3.730517028501505*^9}},ExpressionUUID->"0cb6c15e-85e0-4796-8f20-\
7ebd3ae17ffe"],

