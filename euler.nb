33cb1ff88dff"],

Cell[TextData[{
 "It was proposed by Christian Goldbach that every odd composite number can \
be written as the sum of a prime and twice a square.\n\t\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{"9", "=", 
    RowBox[{"7", "+", 
     RowBox[{"2", "\[Times]", 
      SuperscriptBox["1", "2"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"15", "=", 
    RowBox[{"7", "+", 
     RowBox[{"2", "\[Times]", 
      SuperscriptBox["2", "2"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"21", "=", 
    RowBox[{"3", "+", 
     RowBox[{"2", "\[Times]", 
      SuperscriptBox["3", "2"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"25", "=", 
    RowBox[{"7", "+", 
     RowBox[{"2", "\[Times]", 
      SuperscriptBox["3", "2"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"27", "=", 
    RowBox[{"19", "+", 
     RowBox[{"2", "\[Times]", 
      SuperscriptBox["2", "2"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"33", "=", 
    RowBox[{"31", "+", 
     RowBox[{"2", "\[Times]", 
      SuperscriptBox["1", "2"]}]}]}], TraditionalForm]}],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9e93177c-0c82-44a1-9f50-6112af416d5e"],
 "\nIt turns out that the conjecture was false.\nWhat is the smallest odd \
composite that cannot be written as the sum of a prime and twice a square?"
}], "Text",
 CellChangeTimes->{{3.730354350344445*^9, 
  3.7303544571688023`*^9}},ExpressionUUID->"d29f8b7f-faae-4f03-accf-\
9df07181652c"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", "conjecture", "}"}], ",", "\n", "    ", 
   RowBox[{
    RowBox[{
     RowBox[{"conjecture", "[", 
      RowBox[{"n_", ",", "p_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"n", ">", "p"}], "&&", 
      RowBox[{"IntegerQ", "@", 
       RowBox[{"Sqrt", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "-", "p"}], ")"}], "/", "2"}], "]"}]}]}]}], ";", "\n", 
    "    ", 
    RowBox[{
     RowBox[{"conjecture", "[", "n_", "]"}], ":=", 
     RowBox[{
      RowBox[{"PrimeQ", "[", "n", "]"}], "||", 
      RowBox[{"AnyTrue", "[", 
       RowBox[{
        RowBox[{"Range", "[", "n", "]"}], ",", 
        RowBox[{
         RowBox[{"conjecture", "[", 
          RowBox[{"n", ",", 
           RowBox[{"Prime", "[", "#", "]"}]}], "]"}], "&"}]}], "]"}]}]}], ";",
     "\n", "    ", 
    RowBox[{"NestWhile", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"#", "+", "2"}], "&"}], ",", "35", ",", "conjecture"}], 
     "]"}]}]}], "\n", "]"}]], "Input",
 CellChangeTimes->{{3.7303544696682158`*^9, 3.730354562665901*^9}, 
