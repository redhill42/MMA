     RowBox[{"pandigitalMultiple", "[", "x_", "]"}], ":=", "\n", "        ", 
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"nest", "=", 
         RowBox[{"Function", "[", 
