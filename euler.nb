      RowBox[{"sol", "=", "0"}], ",", 
      RowBox[{"LIMIT", "=", 
       RowBox[{"10", "^", "6"}]}]}], "}"}], ",", "\n", "    ", 
    RowBox[{
     RowBox[{
      RowBox[{"divsum", "[", "n_", "]"}], ":=", 
      RowBox[{
       RowBox[{"DivisorSigma", "[", 
        RowBox[{"1", ",", "n"}], "]"}], "-", "n"}]}], ";", "\n", "\n", "    ", 
     RowBox[{
      RowBox[{"minval", "[", "a_", "]"}], ":=", 
      RowBox[{"minval", "[", 
       RowBox[{"a", ",", 
        RowBox[{"divsum", "[", "a", "]"}], ",", "a"}], "]"}]}], ";", "\n", 
     "    ", 
     RowBox[{
      RowBox[{"minval", "[", 
       RowBox[{"a_", ",", "a_", ",", "m_"}], "]"}], ":=", "m"}], ";", "\n", 
     "    ", 
     RowBox[{
      RowBox[{"minval", "[", 
       RowBox[{"a_", ",", "n_", ",", "m_"}], "]"}], ":=", 
      RowBox[{"minval", "[", 
