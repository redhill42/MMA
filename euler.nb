Cell[TextData[{
 "145 is a curious number, as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"1", "!"}], "+", 
     RowBox[{"4", "!"}], "+", 
     RowBox[{"5", "!"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{"1", "+", "24", "+", "120"}], "=", "145."}]}], TraditionalForm]],
  
  FormatType->"TraditionalForm",ExpressionUUID->
  "9e02aed5-2355-4cca-9527-7f9bc815b8e9"],
 "\nFind the sum of all numbers which are equal to the sum of the factorial \
of their digits.\nNote: as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"1", "!"}], " ", "=", " ", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5126e6d2-d46b-4b4a-8b44-9706023ab68c"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"2", "!"}], " ", "=", " ", "2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "561123dc-dffd-46a5-ac08-36c7182c7b6a"],
 " are not sums they are not included.\nHint: The upper bound in theory \
should be ",
 Cell[BoxData[
  FormBox[
   RowBox[{"7", "\[Times]", 
    RowBox[{"9", "!"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "717b40ed-609a-4881-881b-a85b579088ae"],
 ", but in practical 50000 is enough."
}], "Text",
 CellChangeTimes->{{3.7302967517505913`*^9, 3.73029688827913*^9}, {
  3.730296931472232*^9, 
  3.7302969653350897`*^9}},ExpressionUUID->"7993c7eb-d4a6-4462-af8b-\
9b6438e4d822"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Sum", "[", 
  RowBox[{
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Total", "@", 
       RowBox[{"Factorial", "@", 
        RowBox[{"IntegerDigits", "[", "i", "]"}]}]}], "\[Equal]", "i"}], ",", 
     "i", ",", "0"}], "]"}], ",", 
   RowBox[{"{", 
