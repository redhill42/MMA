     FractionBox["2", 
      RowBox[{
       SqrtBox["23"], "-", "3"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"2", 
        RowBox[{"(", 
         RowBox[{
          SqrtBox["23"], "+", "3"}], ")"}]}], "14"], "=", 
      RowBox[{"1", "+", 
       FractionBox[
        RowBox[{
         SqrtBox["23"], "-", "4"}], "7"]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["a", "3"], "=", "1"}], ",", 
