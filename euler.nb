Cell[CellGroupData[{

Cell[TextData[ButtonBox["44 Pentagon numbers",
 BaseStyle->"Hyperlink",
 ButtonData->{
   URL["https://projecteuler.net/problem=44"], None},
 ButtonNote->"https://projecteuler.net/problem=44"]], "Subsubsection",
 CellChangeTimes->{{3.730498113760285*^9, 3.7304981286729727`*^9}, {
  3.7306263035588427`*^9, 
  3.730626304023109*^9}},ExpressionUUID->"cba57ab1-3a2c-45ab-b22f-\
bedba64adec3"],

Cell[TextData[{
 "Pentagonal numbers are generated by the formula, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "n"], "=", 
    RowBox[{
     RowBox[{"n", "(", 
      RowBox[{
       RowBox[{"3", "n"}], "-", "1"}], ")"}], "/", "2"}]}], TraditionalForm]],
  
  FormatType->"TraditionalForm",ExpressionUUID->
  "98529da0-6f3d-4f0b-8e92-285c9e2ce91e"],
 ". The first ten pentagonal numbers are:\n\t\t1, 5, 12, 22, 35, 51, 70, 92, \
117, 145, ...\nIt can be seen that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "4"], "+", 
     SubscriptBox["P", "7"]}], "=", 
    RowBox[{
     RowBox[{"22", "+", "70"}], "=", 
     RowBox[{"92", "=", 
      SubscriptBox["P", "8"]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cb8c4bd2-5572-4f13-831e-2cdb0f1f07f2"],
 ". However, their difference, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"70", "-", "22"}], "=", "48"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c83328e2-0385-4c7f-ba0c-b0808b6436d2"],
 ", is not pentagonal.\nFind the pair of pentagonal numbers, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", "j"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c5b41e2e-76a5-485b-b339-9cf5c96959ae"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", "k"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e2c307bd-41ac-4b03-b615-39de2f819515"],
 ", for which their sum and difference are pentagonal and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "=", 
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{
      SubscriptBox["P", "k"], "-", 
      SubscriptBox["P", "j"]}], "\[RightBracketingBar]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2abfdf20-1445-486c-abd9-037954730e5c"],
 " is minimised; what is the value of ",
 Cell[BoxData[
  FormBox["D", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "89172d78-7dd5-45d9-bf0a-a98e7603333d"],
 "?\n",
 StyleBox["Unanswered",
  FontColor->RGBColor[1, 0, 0]]
}], "Text",
 CellChangeTimes->{{3.730498133543456*^9, 
  3.7304983216077557`*^9}},ExpressionUUID->"9add3185-57de-46fe-bcae-\
2eddcac92ee3"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[ButtonBox["45 Triangular, pentagonal, and hexagonal",
 BaseStyle->"Hyperlink",
 ButtonData->{
   URL["https://projecteuler.net/problem=45"], None},
 ButtonNote->"https://projecteuler.net/problem=45"]], "Subsubsection",
 CellChangeTimes->{{3.730353416281865*^9, 3.730353433504792*^9}, {
  3.730497612255175*^9, 3.7304976122601337`*^9}, {3.730498245919991*^9, 
  3.7304982473845453`*^9}, {3.730626304758924*^9, 
  3.730626305214676*^9}},ExpressionUUID->"3735f2b3-0cbe-4a51-8d33-\
3fe4d070d4e7"],

Cell[TextData[{
 "Triangle, pentagonal, and hexagonal numbers are generated by the following \
formula:\nTriangle\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "n"], "=", 
    RowBox[{
     RowBox[{"n", "(", 
      RowBox[{"n", "+", "1"}], ")"}], "/", "2"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7e6cbf96-36f1-4254-a46c-289e952a5f7a"],
 "\t\t1, 3, 6, 10, 15, ...\nPentagonal\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "n"], "=", 
    RowBox[{
     RowBox[{"n", "(", 
      RowBox[{
       RowBox[{"3", "n"}], "-", "1"}], ")"}], "/", "2"}]}], TraditionalForm]],
  
  FormatType->"TraditionalForm",ExpressionUUID->
  "0299f025-8071-4b3a-8a01-3f912669e858"],
 "\t1, 5, 12, 22, 35, ...\nHexagonal\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "n"], "=", 
    RowBox[{"n", "(", 
     RowBox[{
      RowBox[{"2", "n"}], "-", "1"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8dc9c1b2-3b1b-4814-a640-b3ca72cdf6b5"],
 "\t\t1, 6, 15, 28, 45, ...\nIt can be verified that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "285"], "=", 
    RowBox[{
     SubscriptBox["P", "165"], "=", 
     RowBox[{
      SubscriptBox["H", "143"], "=", "40755"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ffa7b07e-e305-4d47-86ec-d5d3151e3ade"],
 ".\nFind the next triangle number that is also pentagonal and hexagonal."
}], "Text",
 CellChangeTimes->{{3.730353436693125*^9, 
  3.7303535742326593`*^9}},ExpressionUUID->"5a43e06b-efa2-4e5f-b07a-\
778e13c80d49"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "triangular", ",", "pentagonal", ",", "hexagonal", ",", "triangularQ", 
     ",", "pentagonalQ", ",", "hexagonalQ"}], "}"}], ",", "\n", "    ", 
   RowBox[{
    RowBox[{
     RowBox[{"triangular", "[", "n_", "]"}], ":=", 
     RowBox[{"n", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"n", "+", "1"}], ")"}], "/", "2"}]}]}], ";", "\n", "    ", 
    RowBox[{
     RowBox[{"pentagonal", "[", "n_", "]"}], ":=", 
     RowBox[{"n", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"3", "n"}], "-", "1"}], ")"}], "/", "2"}]}]}], ";", "\n", 
    "    ", 
    RowBox[{
     RowBox[{"hexagonal", "[", "n_", "]"}], ":=", 
     RowBox[{"n", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "-", "1"}], ")"}]}]}], ";", "\n", "  ", "\n", 
    "    ", 
    RowBox[{
     RowBox[{"triangularQ", "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"(", 
        RowBox[{
         SqrtBox[
          RowBox[{
           RowBox[{"8", "x"}], "+", "1"}]], "-", "1"}], ")"}]}], "]"}]}], ";",
     "\n", "    ", 
    RowBox[{
     RowBox[{"pentagonalQ", "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
      RowBox[{
       FractionBox["1", "6"], 
       RowBox[{"(", 
        RowBox[{
         SqrtBox[
          RowBox[{
           RowBox[{"24", "x"}], "+", "1"}]], "+", "1"}], ")"}]}], "]"}]}], 
    ";", "\n", "    ", 
    RowBox[{
     RowBox[{"hexagonalQ", "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
      RowBox[{
       FractionBox["1", "4"], 
       RowBox[{"(", 
        RowBox[{
         SqrtBox[
          RowBox[{
           RowBox[{"8", "x"}], "+", "1"}]], "+", "1"}], ")"}]}], "]"}]}], ";",
     "\n", "  ", "\n", "    ", 
    RowBox[{"triangular", "@", 
     RowBox[{"NestWhile", "[", "\n", "        ", 
      RowBox[{
       RowBox[{
        RowBox[{"#", "+", "1"}], "&"}], ",", "286", ",", 
       RowBox[{
        RowBox[{"!", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"pentagonalQ", "@", 
            RowBox[{"triangular", "[", "#", "]"}]}], "&&", 
           RowBox[{"hexagonalQ", "@", 
            RowBox[{"triangular", "[", "#", "]"}]}]}], ")"}]}], "&"}]}], "\n",
       "    ", "]"}]}]}]}], "\n", "]"}]], "Input",
