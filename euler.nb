Cell[TextData[{
 StyleBox["import",
  FontColor->RGBColor[0, 0, 1]],
 " java.util.stream.IntStream;\n\n",
 StyleBox["public class",
  FontColor->RGBColor[0, 0, 1]],
 " SameDifferences {\n    ",
 StyleBox["static final int",
  FontColor->RGBColor[0, 0, 1]],
 " LIMIT = 1000000;\n\n    ",
 StyleBox["long",
  FontColor->RGBColor[0, 0, 1]],
 " solve() {\n        ",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 "[] solutions = ",
 StyleBox["new int",
  FontColor->RGBColor[0, 0, 1]],
 "[LIMIT + 1];\n        ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " x = 1; x <= LIMIT; x++) {\n            ",
 StyleBox["for",
  FontColor->RGBColor[0, 0, 1]],
 " (",
 StyleBox["int",
  FontColor->RGBColor[0, 0, 1]],
 " y = 1; x * y <= LIMIT; y++) {\n                if ((x + y) % 4 == 0 && 3 * \
y > x) {\n                    solutions[x * y]++;\n                }\n        \
    }\n        }\n        ",
 StyleBox["return",
  FontColor->RGBColor[0, 0, 1]],
 " IntStream.of(solutions).filter(x -> x == 10).count();\n    }\n\n    ",
 StyleBox["public static void",
  FontColor->RGBColor[0, 0, 1]],
 " main(String[] args) {\n        System.out.println(new \
SameDifferences().solve());\n    }\n}"
}], "Program",
