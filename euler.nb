Cell[TextData[ButtonBox["Triangular, pentagonal, and hexagonal",
 BaseStyle->"Hyperlink",
 ButtonData->{
   URL["https://projecteuler.net/problem=45"], None},
 ButtonNote->"https://projecteuler.net/problem=45"]], "Subsubsection",
 CellChangeTimes->{{3.730353416281865*^9, 3.730353433504792*^9}, {
  3.730497612255175*^9, 3.7304976122601337`*^9}, {3.730498245919991*^9, 
  3.7304982473845453`*^9}},ExpressionUUID->"3735f2b3-0cbe-4a51-8d33-\
3fe4d070d4e7"],

Cell[TextData[{
 "Triangle, pentagonal, and hexagonal numbers are generated by the following \
formula:\nTriangle\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "n"], "=", 
    RowBox[{
     RowBox[{"n", "(", 
      RowBox[{"n", "+", "1"}], ")"}], "/", "2"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7e6cbf96-36f1-4254-a46c-289e952a5f7a"],
 "\t\t1, 3, 6, 10, 15, ...\nPentagonal\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "n"], "=", 
    RowBox[{
     RowBox[{"n", "(", 
      RowBox[{
       RowBox[{"3", "n"}], "-", "1"}], ")"}], "/", "2"}]}], TraditionalForm]],
  
  FormatType->"TraditionalForm",ExpressionUUID->
  "0299f025-8071-4b3a-8a01-3f912669e858"],
 "\t1, 5, 12, 22, 35, ...\nHexagonal\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "n"], "=", 
    RowBox[{"n", "(", 
     RowBox[{
      RowBox[{"2", "n"}], "-", "1"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8dc9c1b2-3b1b-4814-a640-b3ca72cdf6b5"],
 "\t\t1, 6, 15, 28, 45, ...\nIt can be verified that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "285"], "=", 
    RowBox[{
     SubscriptBox["P", "165"], "=", 
     RowBox[{
      SubscriptBox["H", "143"], "=", "40755"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ffa7b07e-e305-4d47-86ec-d5d3151e3ade"],
 ".\nFind the next triangle number that is also pentagonal and hexagonal."
}], "Text",
 CellChangeTimes->{{3.730353436693125*^9, 
  3.7303535742326593`*^9}},ExpressionUUID->"5a43e06b-efa2-4e5f-b07a-\
778e13c80d49"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "triangular", ",", "pentagonal", ",", "hexagonal", ",", "triangularQ", 
     ",", "pentagonalQ", ",", "hexagonalQ"}], "}"}], ",", "\n", "    ", 
   RowBox[{
    RowBox[{
     RowBox[{"triangular", "[", "n_", "]"}], ":=", 
     RowBox[{"n", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"n", "+", "1"}], ")"}], "/", "2"}]}]}], ";", "\n", "    ", 
    RowBox[{
     RowBox[{"pentagonal", "[", "n_", "]"}], ":=", 
     RowBox[{"n", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"3", "n"}], "-", "1"}], ")"}], "/", "2"}]}]}], ";", "\n", 
    "    ", 
    RowBox[{
     RowBox[{"hexagonal", "[", "n_", "]"}], ":=", 
     RowBox[{"n", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "-", "1"}], ")"}]}]}], ";", "\n", "  ", "\n", 
    "    ", 
    RowBox[{
     RowBox[{"triangularQ", "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"(", 
        RowBox[{
         SqrtBox[
          RowBox[{
           RowBox[{"8", "x"}], "+", "1"}]], "-", "1"}], ")"}]}], "]"}]}], ";",
     "\n", "    ", 
    RowBox[{
     RowBox[{"pentagonalQ", "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
      RowBox[{
       FractionBox["1", "6"], 
       RowBox[{"(", 
        RowBox[{
         SqrtBox[
          RowBox[{
           RowBox[{"24", "x"}], "+", "1"}]], "+", "1"}], ")"}]}], "]"}]}], 
    ";", "\n", "    ", 
    RowBox[{
     RowBox[{"hexagonalQ", "[", "x_", "]"}], ":=", 
     RowBox[{"IntegerQ", "[", 
      RowBox[{
       FractionBox["1", "4"], 
       RowBox[{"(", 
        RowBox[{
         SqrtBox[
          RowBox[{
           RowBox[{"8", "x"}], "+", "1"}]], "+", "1"}], ")"}]}], "]"}]}], ";",
     "\n", "  ", "\n", "    ", 
    RowBox[{"triangular", "@", 
     RowBox[{"NestWhile", "[", "\n", "        ", 
      RowBox[{
       RowBox[{
        RowBox[{"#", "+", "1"}], "&"}], ",", "286", ",", 
       RowBox[{
        RowBox[{"!", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"pentagonalQ", "@", 
            RowBox[{"triangular", "[", "#", "]"}]}], "&&", 
           RowBox[{"hexagonalQ", "@", 
            RowBox[{"triangular", "[", "#", "]"}]}]}], ")"}]}], "&"}]}], "\n",
       "    ", "]"}]}]}]}], "\n", "]"}]], "Input",
 CellChangeTimes->{{3.730353585356349*^9, 3.730353675561129*^9}, {
