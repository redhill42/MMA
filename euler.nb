 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", "findNum", "}"}], ",", "\n", "    ", 
   RowBox[{
    RowBox[{
     RowBox[{"findNum", "[", 
      RowBox[{
      "t_", ",", "bestN_", ",", "bestD_", ",", "currD_", ",", "minD_"}], 
      "]"}], ":=", "\n", "        ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"currD", "<", "minD"}], ",", "\n", "            ", 
       RowBox[{"bestN", "/", "bestD"}], ",", "\n", "            ", 
       RowBox[{"With", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"currN", "=", 
           RowBox[{"Floor", "[", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"Numerator", "[", "t", "]"}], "*", "currD"}], "-", 
               "1"}], ")"}], "/", 
             RowBox[{"Denominator", "[", "t", "]"}]}], "]"}]}], "}"}], ",", 
         "\n", "                ", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"bestN", "*", "currD"}], "<", 
            RowBox[{"currN", "*", "bestD"}]}], ",", "\n", 
           "                    ", 
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"delta", "=", 
               RowBox[{
                RowBox[{
                 RowBox[{"Numerator", "[", "t", "]"}], "*", "currD"}], "-", 
                RowBox[{
                 RowBox[{"Denominator", "[", "t", "]"}], "*", "currN"}]}]}], 
              "}"}], ",", "\n", "                         ", 
             RowBox[{"findNum", "[", 
              RowBox[{"t", ",", "currN", ",", "currD", ",", 
               RowBox[{"currD", "-", "1"}], ",", 
               RowBox[{"Floor", "[", 
                RowBox[{
                 RowBox[{"currD", "/", "delta"}], "+", "1"}], "]"}]}], 
              "]"}]}], "]"}], ",", "\n", "                     ", 
           RowBox[{"findNum", "[", 
            RowBox[{"t", ",", "bestN", ",", "bestD", ",", 
             RowBox[{"currD", "-", "1"}], ",", "minD"}], "]"}]}], "]"}]}], 
        "]"}]}], "]"}]}], ";", "\n", "    ", 
    RowBox[{
     RowBox[{"findNum", "[", 
      RowBox[{"t_", ",", "n_"}], "]"}], ":=", 
     RowBox[{"findNum", "[", 
      RowBox[{"t", ",", "0", ",", "1", ",", "n", ",", "1"}], "]"}]}], ";", 
    "\n", "    ", 
    RowBox[{"Numerator", "@", 
