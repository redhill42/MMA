Cell["\<\
The primes 3, 7, 109, and 673, are quit remarkable. By taking any two primes \
and concatenating them in any order the result will always be prime. For \
example, taking 7 and 109, both 7109 and 1097 are prime. The sum of these \
four primes, 792, represents the lowest sum for a set of four primes with \
this property.
Find the lowest sum for a set of five primes for which any two primes \
concatenate to produce another prime.\
\>", "Text",
 CellChangeTimes->{{3.730422097135425*^9, 
  3.7304221902236834`*^9}},ExpressionUUID->"1512249d-ac62-4c0e-af59-\
5ca1e682598a"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"primePairSets", "[", "size_", "]"}], ":=", 
    RowBox[{"Module", "[", "\n", "    ", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "primePairQ", ",", "primePairSetQ", ",", "splitPrimePair", ",", 
        "ppset", ",", "buildPrimePairSet"}], "}"}], ",", "\n", "  ", "\n", 
      "    ", 
      RowBox[{
       RowBox[{
        RowBox[{"primePairQ", "[", 
         RowBox[{"{", 
          RowBox[{"xs_List", ",", "ys_List"}], "}"}], "]"}], ":=", "\n", 
        "        ", 
        RowBox[{
         RowBox[{"PrimeQ", "@", 
          RowBox[{"FromDigits", "@", 
           RowBox[{"Join", "[", 
            RowBox[{"xs", ",", "ys"}], "]"}]}]}], "&&", 
         RowBox[{"PrimeQ", "@", 
          RowBox[{"FromDigits", "@", 
           RowBox[{"Join", "[", 
            RowBox[{"ys", ",", "xs"}], "]"}]}]}]}]}], ";", "\n", "    ", 
       RowBox[{
        RowBox[{"primePairQ", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"x_", "?", "PrimeQ"}], ",", 
           RowBox[{"y_", "?", "PrimeQ"}]}], "}"}], "]"}], ":=", "\n", 
        "        ", 
        RowBox[{"primePairQ", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"IntegerDigits", "[", "x", "]"}], ",", 
           RowBox[{"IntegerDigits", "[", "y", "]"}]}], "}"}], "]"}]}], ";", 
       "\n", "    ", 
       RowBox[{
        RowBox[{"primePairQ", "[", "_", "]"}], "=", "False"}], ";", "\n", 
       "  ", "\n", "    ", 
       RowBox[{
        RowBox[{"primePairSetQ", "[", "set_", "]"}], ":=", "\n", "        ", 
        RowBox[{"AllTrue", "[", 
         RowBox[{
          RowBox[{"Subsets", "[", 
           RowBox[{"set", ",", 
            RowBox[{"{", "2", "}"}]}], "]"}], ",", "primePairQ"}], "]"}]}], 
       ";", "\n", "    ", "\n", "    ", 
       RowBox[{
        RowBox[{"splitPrimePair", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", "__", "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "__"}], "}"}]}], "}"}], "]"}], "=", "Nothing"}],
        ";", "\n", "  ", "\n", "    ", 
       RowBox[{
        RowBox[{"splitPrimePair", "[", 
         RowBox[{"{", 
          RowBox[{"xs_List", ",", "ys_List"}], "}"}], "]"}], ":=", "\n", 
        "        ", 
        RowBox[{"With", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"pair", "=", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"FromDigits", "[", "xs", "]"}], ",", 
              RowBox[{"FromDigits", "[", "ys", "]"}]}], "}"}]}], "}"}], ",", 
          "\n", "            ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"primePairQ", "[", "pair", "]"}], ",", "pair", ",", 
            "Nothing"}], "]"}]}], "]"}]}], ";", "\n", "      ", "\n", "    ", 
       
       RowBox[{
        RowBox[{"splitPrimePair", "[", 
         RowBox[{"p_", "?", "PrimeQ"}], "]"}], ":=", "\n", "        ", 
        RowBox[{"With", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"digits", "=", 
            RowBox[{"IntegerDigits", "[", "p", "]"}]}], "}"}], ",", "\n", 
          "            ", 
          RowBox[{"Map", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"splitPrimePair", "@", 
              RowBox[{"TakeDrop", "[", 
               RowBox[{"digits", ",", "#"}], "]"}]}], "&"}], ",", 
            RowBox[{"Range", "[", 
             RowBox[{"1", ",", 
              RowBox[{
               RowBox[{"Length", "[", "digits", "]"}], "-", "1"}]}], "]"}]}], 
           "]"}]}], "]"}]}], ";", "\n", "  ", "\n", "    ", 
       RowBox[{
        RowBox[{"splitPrimePair", "[", "_", "]"}], "=", "Nothing"}], ";", 
       "\n", "  ", "\n", "    ", 
       RowBox[{
        RowBox[{"ppset", "[", "_", "]"}], "=", 
        RowBox[{"{", "}"}]}], ";", "\n", "  ", "\n", "    ", 
       RowBox[{
        RowBox[{"buildPrimePairSet", "[", 
         RowBox[{"pair", ":", 
          RowBox[{"{", 
           RowBox[{"x_", ",", "y_"}], "}"}]}], "]"}], ":=", 
        RowBox[{"(", "\n", "        ", 
         RowBox[{
          RowBox[{
           RowBox[{"ppset", "[", "x", "]"}], "=", 
           RowBox[{"Map", "[", "\n", "            ", 
            RowBox[{
             RowBox[{
              RowBox[{"Which", "[", "\n", "                ", 
               RowBox[{
                RowBox[{"MemberQ", "[", 
                 RowBox[{"#", ",", "y"}], "]"}], ",", "\n", 
                "                    ", "#", ",", "\n", "                ", 
                RowBox[{"primePairSetQ", "[", 
                 RowBox[{"Join", "[", 
                  RowBox[{"#", ",", "pair"}], "]"}], "]"}], ",", "\n", 
                "                    ", 
                RowBox[{"If", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"Length", "[", "#", "]"}], "\[Equal]", 
                   RowBox[{"size", "-", "2"}]}], ",", "\n", 
                  "                        ", 
                  RowBox[{"Throw", "[", 
                   RowBox[{"Join", "[", 
                    RowBox[{"#", ",", "pair"}], "]"}], "]"}], ",", "\n", 
                  "                        ", 
                  RowBox[{"Append", "[", 
                   RowBox[{"#", ",", "y"}], "]"}]}], "]"}], ",", "\n", 
                "                ", "True", ",", "\n", "                    ",
                 "#"}], "\n", "          ", "]"}], "&"}], ",", 
             RowBox[{"ppset", "[", "x", "]"}]}], "]"}]}], ";", "\n", 
          "          ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"NoneTrue", "[", 
             RowBox[{
              RowBox[{"ppset", "[", "x", "]"}], ",", 
              RowBox[{
               RowBox[{"MemberQ", "[", 
                RowBox[{"#", ",", "y"}], "]"}], "&"}]}], "]"}], ",", "\n", 
            "              ", 
            RowBox[{
             RowBox[{"ppset", "[", "x", "]"}], "=", 
             RowBox[{"Append", "[", 
              RowBox[{
               RowBox[{"ppset", "[", "x", "]"}], ",", 
               RowBox[{"{", "y", "}"}]}], "]"}]}]}], "\n", "          ", 
           "]"}], ";"}], "\n", "    ", ")"}]}], ";", "\n", "  ", "\n", "    ", 
       RowBox[{
        RowBox[{"buildPrimePairSet", "[", 
         RowBox[{"p_", "?", "PrimeQ"}], "]"}], ":=", "\n", "        ", 
        RowBox[{"Scan", "[", "\n", "            ", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"buildPrimePairSet", "[", "#", "]"}], ";", 
             RowBox[{"buildPrimePairSet", "[", 
              RowBox[{"Reverse", "[", "#", "]"}], "]"}]}], ")"}], "&"}], ",", 
          "\n", "            ", 
          RowBox[{"splitPrimePair", "[", "p", "]"}]}], "\n", "        ", 
         "]"}]}], ";", "\n", "    ", "\n", "    ", 
       RowBox[{"Catch", "@", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{"buildPrimePairSet", "[", 
           RowBox[{"Prime", "[", "i", "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "Infinity"}], "}"}]}], "]"}]}]}]}], "\n", 
     "]"}]}], ";"}], "\n"}], "\n", 
 RowBox[{"Total", "@", 
  RowBox[{"primePairSets", "[", "5", "]"}]}]}], "Input",
 CellChangeTimes->{{3.730422974045422*^9, 3.730422976975375*^9}, {
  3.730423157183689*^9, 3.730423158591481*^9}, {3.730515676660727*^9, 
  3.730516247037448*^9}, {3.730518038862918*^9, 
  3.730518085549511*^9}},ExpressionUUID->"b258b802-12f8-45bb-8a8c-\
c7aa332bdc95"],

